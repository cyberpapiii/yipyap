		"lucide-svelte": "^0.544.0",
		"postcss": "^8.5.6",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.13"
	},
	"overrides": {
		"@rollup/rollup-linux-x64-gnu": "npm:@rollup/rollup-linux-x64-gnu@latest"
	}
}
